// Copyright Year
var d = new Date(),
    n = d.getFullYear();
document.getElementById("year").innerHTML = n;


$(document).ready(function(){
    // Window Height
    var windowHeight=$(window).height();
    $(window).height(windowHeight+"px");

    var windowWidth=$(window).width();
    //alert(windowWidth);

    // Carousel Photos
    $("#myCarousel").carousel({interval: 2000});

});
